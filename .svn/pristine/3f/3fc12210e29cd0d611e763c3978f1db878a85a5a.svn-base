package com.zp.jdb.entity;

public class Img_forhouse {
	
	private int id;
	
	private String img;
	
	private int houseID;
	
	private String order_number;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getImg() {
		return img;
	}

	public void setImg(String img) {
		this.img = img;
	}

	public int getHouseID() {
		return houseID;
	}

	public void setHouseID(int houseID) {
		this.houseID = houseID;
	}

	public String getOrder_number() {
		return order_number;
	}

	public void setOrder_number(String order_number) {
		this.order_number = order_number;
	}

	@Override
	public String toString() {
		return "Img_forhouse [id=" + id + ", img=" + img + ", houseID=" + houseID + ", order_number=" + order_number
				+ ", getId()=" + getId() + ", getImg()=" + getImg() + ", getHouseID()=" + getHouseID()
				+ ", getOrder_number()=" + getOrder_number() + ", getClass()=" + getClass() + ", hashCode()="
				+ hashCode() + ", toString()=" + super.toString() + "]";
	}
	
	

}
